package Strings;

public class ReverseAString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str = "dharitree%^&**sahoo";
		String strREv;
		int len =  str.length();
		for(int i=len-1;i>=0;i--)
		{
		char ch =  str.charAt(i);
		if(ch>'a' || ch<'z')
		{
			strREv = strREv.concat(ch)
		}
		}

	}

}
